{
  "dashboard": {
    "alerting": {
      "conditions": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "execErrState": "Alerting",
          "frequency": "1m",
          "handler": 1,
          "id": 1,
          "message": "More than 5 anomalies detected in the last 5 minutes",
          "name": "High Anomaly Rate",
          "noDataState": "NoData",
          "query": {
            "expr": "sum(increase(anomalies_detected_total[5m])) > 5",
            "instant": true,
            "refId": "A"
          }
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "execErrState": "Alerting",
          "frequency": "5m",
          "handler": 1,
          "id": 2,
          "message": "Skill completion rate below 70%",
          "name": "Low Completion Rate",
          "noDataState": "NoData",
          "query": {
            "expr": "avg(skill_completion_rate) by (skill_name) < 0.7",
            "instant": true,
            "refId": "A"
          }
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "execErrState": "Alerting",
          "frequency": "30s",
          "handler": 1,
          "id": 3,
          "message": "WebSocket server is not responding",
          "name": "WebSocket Server Down",
          "noDataState": "Alerting",
          "query": {
            "expr": "up{job=\"session-buddy-websocket\"} == 0",
            "instant": true,
            "refId": "A"
          }
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "execErrState": "Alerting",
          "frequency": "1m",
          "handler": 1,
          "id": 4,
          "message": "Database connection pool usage above 90%",
          "name": "Database Pool Exhausted",
          "noDataState": "NoData",
          "query": {
            "expr": "sqlite_connections_active / sqlite_connections_max > 0.9",
            "instant": true,
            "refId": "A"
          }
        }
      ]
    },
    "description": "Real-time monitoring for skill metrics, anomalies, and system health",
    "panels": [
      {
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "fillOpacity": 10,
              "lineWidth": 2
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "id": 1,
        "targets": [
          {
            "expr": "rate(skill_invocations_total[1m])",
            "legendFormat": "{{skill_name}} ({{workflow_phase}})",
            "refId": "A"
          }
        ],
        "title": "Real-Time Skill Invocations",
        "type": "timeseries"
      },
      {
        "fieldConfig": {
          "defaults": {
            "max": 1,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.7
                },
                {
                  "color": "green",
                  "value": 0.9
                }
              ]
            },
            "unit": "percentunit"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "id": 2,
        "targets": [
          {
            "expr": "avg(skill_completion_rate) by (skill_name)",
            "legendFormat": "{{skill_name}}",
            "refId": "A"
          }
        ],
        "title": "Skill Completion Rate",
        "type": "gauge"
      },
      {
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "id": 3,
        "targets": [
          {
            "expr": "sum(increase(anomalies_detected_total[5m]))",
            "legendFormat": "Anomalies (5m)",
            "refId": "A"
          }
        ],
        "title": "Anomaly Detection",
        "type": "stat"
      },
      {
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "text": "No Sessions"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 4
        },
        "id": 4,
        "targets": [
          {
            "expr": "active_sessions_total",
            "legendFormat": "Active Sessions",
            "refId": "A"
          }
        ],
        "title": "Active Sessions",
        "type": "stat"
      },
      {
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "id": 5,
        "targets": [
          {
            "expr": "topk(10, sum(increase(skill_invocations_total[24h])) by (skill_name))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "title": "Top Skills by Invocations (24h)",
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {
                "Value": 1,
                "skill_name": 0
              },
              "renameByName": {
                "Value": "Invocations",
                "skill_name": "Skill Name"
              }
            }
          }
        ],
        "type": "table"
      },
      {
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "spectrum"
            },
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "id": 6,
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(skill_duration_seconds_bucket[5m])) by (le, skill_name))",
            "legendFormat": "{{skill_name}}",
            "refId": "A"
          }
        ],
        "title": "Skill Duration Distribution",
        "type": "heatmap"
      },
      {
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [
              {
                "options": {
                  "cleanup": {
                    "text": "Cleanup"
                  },
                  "execution": {
                    "text": "Execution"
                  },
                  "rollback": {
                    "text": "Rollback"
                  },
                  "setup": {
                    "text": "Setup"
                  },
                  "verification": {
                    "text": "Verification"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "id": 7,
        "targets": [
          {
            "expr": "sum(increase(skill_invocations_total[1h])) by (workflow_phase)",
            "legendFormat": "{{workflow_phase}}",
            "refId": "A"
          }
        ],
        "title": "Workflow Phase Distribution",
        "type": "piechart"
      },
      {
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 3
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "id": 8,
        "targets": [
          {
            "expr": "increase(anomalies_detected_total[1h])",
            "legendFormat": "{{anomaly_type}} - {{skill_name}}",
            "refId": "A"
          }
        ],
        "title": "Anomaly Timeline",
        "type": "timeline"
      },
      {
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "text": "DOWN"
                  },
                  "1": {
                    "color": "green",
                    "text": "UP"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 24
        },
        "id": 9,
        "targets": [
          {
            "expr": "up{job=\"session-buddy-websocket\"}",
            "legendFormat": "Server Status",
            "refId": "A"
          }
        ],
        "title": "WebSocket Server Health",
        "type": "stat"
      },
      {
        "fieldConfig": {
          "defaults": {
            "max": 1,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.7
                },
                {
                  "color": "red",
                  "value": 0.9
                }
              ]
            },
            "unit": "percentunit"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 24
        },
        "id": 10,
        "targets": [
          {
            "expr": "sqlite_connections_active / sqlite_connections_max",
            "legendFormat": "Pool Usage",
            "refId": "A"
          }
        ],
        "title": "Database Connection Pool",
        "type": "gauge"
      },
      {
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 12,
          "x": 12,
          "y": 24
        },
        "id": 11,
        "targets": [
          {
            "expr": "sum(ALERTS{alertname=~\"SessionBuddy.*\"}) by (alertstate)",
            "legendFormat": "{{alertstate}}",
            "refId": "A"
          }
        ],
        "title": "Alert Rules Summary",
        "type": "stat"
      }
    ],
    "refresh": "5s",
    "schemaVersion": 38,
    "tags": [
      "session-buddy",
      "skills",
      "analytics",
      "phase4"
    ],
    "timezone": "browser",
    "title": "Session-Buddy Phase 4 Monitoring",
    "version": 1
  }
}
